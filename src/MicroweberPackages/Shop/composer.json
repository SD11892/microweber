{
  "name": "microweber-packages/microweber-shop",
  "description": "Microweber Shop.",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Microweber",
      "email": "info@microweber.com"
    },
    {
      "name": "Bozhidar Slaveykov",
      "email": "bobi@microweber.com"
    }
  ],
  "require": {
    "twig/twig": "3.x-dev"
  },
  "require-dev": {

  },
  "autoload": {
    "psr-4": {
      "MicroweberPackages\\Shop\\": "src/"
    },
    "files": [
      "src/ShopManager/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\Shop\\CartManager\\CartManagerServiceProvider",
        "MicroweberPackages\\Shop\\CheckoutManager\\CheckoutManagerServiceProvider",
        "MicroweberPackages\\Shop\\ClientsManager\\ClientsManagerServiceProvider",
        "MicroweberPackages\\Shop\\InvoicesManager\\InvoicesManagerServiceProvider",
        "MicroweberPackages\\Shop\\OrderManager\\OrderManagerServiceProvider",
        "MicroweberPackages\\Shop\\ShopManager\\ShopManagerServiceProvider",
        "MicroweberPackages\\Shop\\TaxManager\\TaxManagerServiceProvider"
      ],
      "aliases": {
        "CatManager": "MicroweberPackages\\Shop\\CatManager\\CartManagerFacade",
        "CheckoutManager": "MicroweberPackages\\Shop\\CheckoutManager\\CheckoutManagerFacade",
        "ClientsManager": "MicroweberPackages\\Shop\\ClientsManager\\ClientsManagerFacade",
        "InvoicesManager": "MicroweberPackages\\Shop\\InvoicesManager\\InvoicesManagerFacade",
        "OrderManager": "MicroweberPackages\\Shop\\OrderManager\\OrderManagerFacade",
        "ShopManager": "MicroweberPackages\\Shop\\ShopManager\\ShopManagerFacade",
        "TaxManager": "MicroweberPackages\\Shop\\TaxManager\\TaxManagerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "http://microweber.com",
  "license": "MIT",
  "support": {
    "email": "support@microweber.com"
  }
}
