{
    "name": "microweber-packages/laravel-config-save",
    "description": "Config rewrite for Laravel. Allows to save key and values in cofig files.",
    "version" : "0.0.1",
    "authors": [
        {
            "name": "Microweber",
            "email": "info@microweber.com"
        },
		{
            "name": "Bozhidar Slaveykov",
            "email": "bobi@microweber.com"
        }
  ],
  "require": {
        "microweber-packages/helper-functions": "*"
    },
  "autoload": {
      "psr-4": {
        "MicroweberPackages\\": "src/"
      }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\Config\\ConfigSaveServiceProvider"
      ],
      "aliases": {
        "ConfigSave": "MicroweberPackages\\Config\\ConfigSaveFacade"
      }
    }
  },
  "homepage": "http://microweber.com",
