{
  "name": "microweber-packages/microweber-event-manager",
  "description": "Microweber event manager.",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Microweber",
      "email": "info@microweber.com"
    },
    {
      "name": "Bozhidar Slaveykov",
      "email": "bobi@microweber.com"
    }
  ],
  "require": {

    "illuminate/database": "*",
    "jeremeamia/superclosure": "*",
    "microweber-packages/microweber-helpers": "dev-master"
  },

  "autoload": {
    "psr-4": {
      "MicroweberPackages\\Core\\EventManager\\": "src/EventManager/"
    },
    "files": [
      "src/EventManager/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\Core\\EventManager\\EventManagerServiceProvider"
      ],
      "aliases": {
        "EventManager": "MicroweberPackages\\Core\\EventManager\\EventManagerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "http://microweber.com",
  "license": "MIT",
  "support": {
    "email": "support@microweber.com"
  }
}

