{
  "name": "microweber-packages/microweber-forms-manager",
  "description": "Microweber forms manager.",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Microweber",
      "email": "info@microweber.com"
    },
    {
      "name": "Bozhidar Slaveykov",
      "email": "bobi@microweber.com"
    }
  ],
  "require": {
    "microweber-packages/microweber-helpers": "dev-master"
  },

  "autoload": {
    "psr-4": {
      "MicroweberPackages\\": "src/"
    },
    "files": [
      "src/FormsManager/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\FormsManager\\FormsManagerServiceProvider"
      ],
      "aliases": {
        "FormsManager": "MicroweberPackages\\FormsManager\\FormsManagerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "http://microweber.com",
  "license": "MIT",
  "support": {
    "email": "support@microweber.com"
  }
}
