{
  "name": "microweber-packages/microweber-cms",
  "description": "Microweber CMS.",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Microweber",
      "email": "info@microweber.com"
    },
    {
      "name": "Bozhidar Slaveykov",
      "email": "bobi@microweber.com"
    }
  ],
  "require": {
    "jeremeamia/superclosure": "*",
    "rtconner/laravel-tagging": "^4.0",
    "knplabs/knp-menu": "^3.1",
    "microweber-packages/microweber-database-manager": "dev-master",
    "microweber-packages/microweber-event-manager": "dev-master",
    "microweber-packages/microweber-helpers": "dev-master"
  },

  "autoload": {
    "psr-4": {
      "MicroweberPackages\\": "src/",
      "MicroweberPackages\\Content\\CategoryManager\\": "src/CategoryManager/",
      "MicroweberPackages\\Content\\TagsManager\\": "src/TagsManager/"
    },
    "files": [
      "src/ContentManager/helpers.php",
      "src/CategoryManager/helpers.php",
      "src/MediaManager/helpers.php",
      "src/MediaManager/helpers_paths.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MicroweberPackages\\Content\\ContentManager\\ContentManagerServiceProvider",
        "MicroweberPackages\\Content\\CategoryManager\\CategoryManagerServiceProvider",
        "MicroweberPackages\\Content\\TagsManager\\TagsManagerServiceProvider",
        "MicroweberPackages\\Content\\MediaManager\\MediaManagerServiceProvider",
        "MicroweberPackages\\Content\\MenuManager\\MenuManagerServiceProvider"
      ],
      "aliases": {
        "ContentManager": "MicroweberPackages\\Content\\ContentManager\\ContentManagerFacade",
        "CategoryManager": "MicroweberPackages\\Content\\CategoryManager\\CategoryManagerFacade",
        "MediaManager": "MicroweberPackages\\Content\\MediaManager\\MediaManagerFacade",
        "MenuManager": "MicroweberPackages\\Content\\MenuManager\\MenuManagerFacade",
        "TagsManager": "MicroweberPackages\\Content\\TagsManager\\TagsManagerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "http://microweber.com",
  "license": "MIT",
  "support": {
    "email": "support@microweber.com"
  }
}
